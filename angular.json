{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "bs-ngx-pagination": {
      "root": "bs-ngx-pagination",
      "sourceRoot": "bs-ngx-pagination/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "bs-ngx-pagination/tsconfig.lib.json",
            "project": "bs-ngx-pagination/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "bs-ngx-pagination/src/test.ts",
            "tsConfig": "bs-ngx-pagination/tsconfig.spec.json",
            "karmaConfig": "bs-ngx-pagination/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "bs-ngx-pagination/tsconfig.lib.json",
              "bs-ngx-pagination/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "bs-ngx-pagination"
}
